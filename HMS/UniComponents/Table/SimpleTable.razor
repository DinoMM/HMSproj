@namespace UniComponents
<table class="table table-striped table-hover">
    <thead>
        <tr>
            @THEAD
        </tr>
    </thead>
    <tbody>
        @TBODY
    </tbody>
</table>

@code {
    [Parameter]
    public RenderFragment THEAD { get; set; }

    [Parameter]
    public RenderFragment TBODY { get; set; }


}

